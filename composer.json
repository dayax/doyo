{
    "name": "dayax/doyo",
    "description": "The Twitter Bootstrap WordPress Theme",
    "license": "MIT",
    "type": "wordpress-theme",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "doyo": ["lib/","tests/unit/"]
        }
    },
    "require": {
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "twitter/bootstrap": "2.3.x",
        "fortawesome/font-awesome": "3.2.x",
        "leafo/lessphp": "0.3.x",
        "tedivm/jshrink": "*",
        "symfony/finder": "2.3.x",
        "symfony/filesystem": "2.3.x",
        
        "wordpress/wordpress": "3.5.2",
        "dayax/wordpress-test": "@dev"
    },
    "extra": {
        "installer-paths": {
            "wp-content/themes/{$name}": ["type:wordpress-theme"]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/"
    }
}